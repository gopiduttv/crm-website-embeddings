# Server Configuration
PORT=5000

# Google Custom Search API (Primary Discovery Method)
# Get your keys from: https://console.cloud.google.com/
# GOOGLE_API_KEY=your_google_api_key_here
# GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# Bing Search API (Secondary Discovery Method)
# Get your key from: https://portal.azure.com/
# BING_API_KEY=your_bing_api_key_here

# Database (Optional)
# DATABASE_URL=postgresql://user:password@localhost:5432/crm_tracker

# CORS Configuration
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info

# Discovery Settings (Optional)
# MAX_DISCOVERY_RESULTS=50
# DISCOVERY_TIMEOUT=30000
# SCRAPING_ENABLED=true
